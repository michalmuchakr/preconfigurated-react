{
  "name": "preconfigurated-react",
  "version": "0.1.0",
  "description": "Welcome to React pre configured environment.",
  "main": "src/electron-entry.js",
  "homepage": "./",
  "scripts": {
    "build": "npm run clean && webpack --config webpack.config.prod.js",
    "build-storybook": "build-storybook",
    "clean": "shx rm -rf ./dist",
    "electron": "electron .",
    "electron-dev": "set ELECTRON_START_URL=http://localhost:1234 && electron .",
    "start": "electron-forge start",
    "start:dev": "webpack serve --config webpack.config.dev.js --progress --profile",
    "lint": "eslint src/**/*.js src/**/*.jsx --fix",
    "prettier": "prettier --write .",
    "test": "jest",
    "test:coverage": "jest --collect-coverage",
    "test:watch": "jest --watchAll",
    "storybook": "start-storybook -p 6006",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "author": "Michal Mucha | General Cube",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/michalmuchakr/preconfigurated-react"
  },
  "devDependencies": {
    "@babel/cli": "7.13.10",
    "@babel/core": "7.13.10",
    "@babel/eslint-parser": "7.13.10",
    "@babel/preset-env": "7.13.10",
    "@babel/preset-react": "7.12.13",
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "@storybook/addon-actions": "6.1.21",
    "@storybook/addon-essentials": "6.1.21",
    "@storybook/addon-links": "6.1.21",
    "@storybook/react": "6.1.21",
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/react": "11.2.5",
    "@testing-library/user-event": "12.8.3",
    "axios-mock-adapter": "1.19.0",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.2",
    "babel-plugin-module-resolver": "4.1.0",
    "concurrently": "^6.0.1",
    "css-loader": "5.1.3",
    "electron": "^12.0.1",
    "eslint": "7.22.0",
    "eslint-config-google": "0.14.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.3.1",
    "husky": "5.1.3",
    "jest": "26.6.3",
    "mini-css-extract-plugin": "1.3.9",
    "node-sass": "5.0.0",
    "prettier": "2.2.1",
    "react-test-renderer": "17.0.1",
    "resolve-url-loader": "^3.1.2",
    "sass-loader": "11.0.1",
    "shx": "0.3.3",
    "style-loader": "2.0.0",
    "webpack": "5.26.0",
    "webpack-assets-manifest": "5.0.1",
    "webpack-cli": "4.5.0",
    "webpack-dev-server": "3.11.2"
  },
  "dependencies": {
    "axios": "0.21.1",
    "electron-squirrel-startup": "^1.0.0",
    "express": "4.17.1",
    "prop-types": "15.7.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-helmet": "6.1.0",
    "react-router-dom": "5.2.0"
  },
  "browserslist": [
    "defaults",
    "IE 11"
  ],
  "config": {
    "forge": "./forge.config.js"
  }
}
