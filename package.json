{
  "name": "preconfigurated-react",
  "version": "0.1.0",
  "description": "Welcome to React pre configured environment.",
  "main": "index.jsx",
  "scripts": {
    "build": "npm run clean && webpack --config webpack.config.prod.js",
    "build-storybook": "build-storybook",
    "clean": "shx rm -rf ./dist",
    "start": "node express-static.js",
    "start:dev": "webpack serve --config webpack.config.dev.js --progress --profile",
    "lint": "eslint src/**/*.js src/**/*.jsx --fix",
    "prettier": "prettier --write .",
    "test": "jest",
    "test:coverage": "jest --collect-coverage",
    "test:watch": "jest --watchAll",
    "storybook": "start-storybook -p 6006"
  },
  "author": "Michal Mucha | General Cube",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/michalmuchakr/preconfigurated-react"
  },
  "devDependencies": {
    "@babel/cli": "7.13.14",
    "@babel/core": "7.13.15",
    "@babel/eslint-parser": "7.13.14",
    "@babel/preset-env": "7.13.15",
    "@babel/preset-react": "7.13.13",
    "@storybook/addon-actions": "6.2.7",
    "@storybook/addon-essentials": "6.2.7",
    "@storybook/addon-links": "6.2.7",
    "@storybook/react": "6.2.7",
    "@testing-library/jest-dom": "5.11.10",
    "@testing-library/react": "11.2.6",
    "@testing-library/user-event": "13.1.2",
    "axios-mock-adapter": "1.19.0",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.2",
    "babel-plugin-module-resolver": "4.1.0",
    "css-loader": "5.2.1",
    "eslint": "7.24.0",
    "eslint-config-google": "0.14.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.23.2",
    "eslint-plugin-react-hooks": "4.2.0",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.3.1",
    "husky": "6.0.0",
    "jest": "26.6.3",
    "mini-css-extract-plugin": "1.4.1",
    "node-sass": "5.0.0",
    "prettier": "2.2.1",
    "react-test-renderer": "17.0.2",
